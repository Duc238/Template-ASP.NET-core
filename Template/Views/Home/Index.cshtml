@using Data
@model IEnumerable<HocSinhDetail>;
@{
    ViewData["Title"] = "Home Page";
}
@if(Model!=null)
{
    <table class="table table-hover">
        <tr>
            <th>STT</th>
            <th>Tên</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </table>
}
else
{
    <p>No haven't data</p>
}
